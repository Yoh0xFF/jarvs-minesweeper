{
  "name": "jarvs-minesweeper",
  "description": "JARVS(Just A Rather Very Simple) Minesweeper",
  "version": "1.0.0",
  "author": "Ioram Gordadze <Yoh0xFF@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/Yoh0xFF/jarvs-minesweeper",
  "private": true,
  "type": "module",
  "homepage": ".",
  "scripts": {
    "start": "vite",
    "start:preview": "vite preview",
    "build": "tsc && vite build",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "format": "prettier --write 'src/**/*.ts' 'src/**/*.tsx' 'src/**/*.scss'",
    "deploy": "gh-pages -d dist -u 'github-actions-bot <support+actions@github.com>'",
    "clean": "npx rimraf ./**/dist ./**/node_modules"
  },
  "dependencies": {
    "canvas-confetti": "1.9.4",
    "classnames": "2.5.1",
    "react": "19.2.4",
    "react-dom": "19.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/react": "19.2.14",
    "@types/canvas-confetti": "1.9.0",
    "@types/node": "25.3.2",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.4",
    "jsdom": "28.1.0",
    "prettier": "3.8.1",
    "prettier-plugin-organize-imports": "4.3.0",
    "prettier-plugin-packagejson": "3.0.0",
    "sass": "1.97.3",
    "typescript": "5.9.3",
    "vitest": "4.0.18",
    "vite": "7.3.1",
    "gh-pages": "6.3.0",
    "web-vitals": "5.1.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.59.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}